link {
    modem_id: $modem_id
    subnet_mask: $subnet_mask
    driver {
        driver_type: DRIVER_UDP_MULTICAST
        [goby.acomms.udp_multicast.protobuf.config] {  
            multicast_address: "239.142.0.2"
            multicast_port: 54500
            max_frame_size: 1400
        }           
    }
    mac {
        type: MAC_FIXED_DECENTRALIZED
        slot { src: 1 slot_seconds: 5 max_frame_bytes: 64 }
        slot { src: 2 slot_seconds: 5 max_frame_bytes: 64 }
    }        
}
